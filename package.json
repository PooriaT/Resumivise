{
  "name": "resumivise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "cd api && python3 -m venv venv && . venv/bin/activate && pip install -r requirements.txt && uvicorn main:app --reload --workers 4",
    "fastapi-start": "cd api && python3 -m venv venv && . venv/bin/activate && pip install -r requirements.txt && uvicorn main:app --host 0.0.0.0 --port 8000 --workers 4",
    "next-dev": "next dev",
    "next-start": "next start",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "concurrently \"npm run next-start\" \"npm run fastapi-start\"",
    "lint": "next lint"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "next": "14.0.2",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.2",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
